{
  "version": "0.0.1",
  "description": "A React component that renders any number of pregenerated HTMLImageElements that move about randomly.",
  "name": "react-icon-ether",
  "license": "MIT",
  "main": "dist/react-icon-ether.js",
  "module": "dist/react-icon-ether.mjs",
  "types": "dist/react-icon-ether.d.ts",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "build:cjs": "tsc --module commonjs --outDir build/dist",
    "build:esm": "tsc --target es2018 --outDir build/esm",
    "build:tsup": "tsup --onSuccess \"node dist/index.js\"",
    "build": "yarn build:tsup",
    "preversion": "yarn build",
    "postversion": "git push --follow-tags",
    "test": "jest",
    "lint": "tsc",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "author": {
    "name": "Adele Beitvashahi",
    "email": "adelbeit@gmail.com",
    "url": "https://adelbeit.com"
  },
  "keywords": [
    "react component",
    "react-component",
    "react",
    "icon ether",
    "iconether",
    "icon-ether",
    "image ether",
    "imageether",
    "image-ether",
    "floating images",
    "floatingimages",
    "floating-images",
    "canvas",
    "react image",
    "reactimage",
    "react-image",
    "particle canvas",
    "particle-canvas",
    "particlecanvas"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adelbeit/react-icon-ether"
  },
  "bugs": {
    "url": "https://github.com/adelbeit/react-icon-ether/issues"
  },
  "homepage": "https://github.com/adelbeit/react-icon-ether",
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "20 KB"
    }
  ],
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@ladle/react": "^2.12.2",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "esbuild": "^0.17.17",
    "jest": "^29.5.0",
    "postcss": "^8.4.23",
    "postcss-modules": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "size-limit": "^8.2.4",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
