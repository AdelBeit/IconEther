{
  "version": "0.0.1",
  "description": "A React component that renders any number of pregenerated HTMLImageElements that move about randomly.",
  "name": "react-icon-ether",
  "license": "MIT",
  "author": {
    "name": "Adele Beitvashahi",
    "email": "adelbeit@gmail.com",
    "url": "https://adelbeit.com"
  },
  "keywords": [
    "react component",
    "react-component",
    "react",
    "icon ether",
    "iconether",
    "icon-ether",
    "image ether",
    "imageether",
    "image-ether",
    "floating images",
    "floatingimages",
    "floating-images",
    "canvas",
    "react image",
    "reactimage",
    "react-image",
    "particle canvas",
    "particle-canvas",
    "particlecanvas"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adelbeit/react-icon-ether"
  },
  "bugs": {
    "url": "https://github.com/adelbeit/react-icon-ether/issues"
  },
  "homepage": "https://github.com/adelbeit/react-icon-ether",
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "20 KB"
    }
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "dev:tsup": "tsup src/index.ts --format esm,cjs -d dist --dts --external react --onSuccess \"node dist/index.js\" --watch",
    "dev:css": "postcss src/**/*.module.css -o public/css/main.css --watch",
    "build:tsup": "tsup src/index.ts --format esm,cjs -d dist --dts",
    "build:css": "postcss src/**/*.module.css -o public/css/main.css",
    "dev": "vite",
    "build": "concurrently \"tsc\" \"vite build\"",
    "preversion": "yarn build",
    "preview": "vite preview",
    "postversion": "git push --follow-tags",
    "test": "jest",
    "lint": "tsc",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.3.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "jest": "^29.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^8.2.4",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.3",
    "vite-plugin-dts": "^2.3.0"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
